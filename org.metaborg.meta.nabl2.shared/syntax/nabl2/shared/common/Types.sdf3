module nabl2/shared/common/Types

imports

  nabl2/shared/common/Native

context-free syntax

context-free syntax

  TypeTerm   = TypeList
  Comparable = TypeTerm

  TypeList.TList     = <[<{TypeTerm ","}*>]>
  TypeList.TListTail = <[<{TypeTerm ","}*>|<TypeTerm>]>
  // needs to be TypeTerm, because indirection via TypeList
  // makes {left} not work
  TypeTerm.TListConc = <<TypeTerm> ++ <TypeTerm>> {left}

lexical syntax

  Constructor = "TList" | "TListTail" | "TListConc"
