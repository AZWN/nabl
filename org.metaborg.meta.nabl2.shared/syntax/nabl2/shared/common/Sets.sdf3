module nabl2/shared/common/Sets

imports

  nabl2/shared/common/CTerms

context-free syntax

  SetExpr.EmptySet = <0>                              // FIXME Use NaBL2_ prefix
  SetExpr.Isect    = <(<CTerm> isect <CTerm>)> {left} // FIXME Use NaBL2_ prefix
  SetExpr.Union    = <(<CTerm> union <CTerm>)> {left} // FIXME Use NaBL2_ prefix
  SetExpr.Diff     = <(<CTerm> minus <CTerm>)> {left} // FIXME Use NaBL2_ prefix

  CTerm = SetExpr
