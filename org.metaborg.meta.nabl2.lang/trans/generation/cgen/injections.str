module generation/cgen/injections

imports

  nabl2shared
  
  signatures/common/-
  signatures/rules/-
  signatures/nabl2/shared/constraints/-
  generation/cgen/-
  generation/-
  util/-
 
rules

  injection-to-term(e) = term-to-term(e)
  injection-to-term(e):
    CWld() -> Term |[ <nabl2--new-var(|~<source-var>);try(nabl2--copy-origin(|~<term-var>))> ]|
  injection-to-term(e):
    TermIndexFrom(t) -> Term |[ <nabl2--get-ast-index> ~t ]|
  injection-to-term(e):
    NAMEWithOrigin() -> Term |[ <try(nabl2--copy-origin(|~<term-var>))> NAME() ]|
  injection-to-term(e):
    OccurrenceIndexFrom(t) -> Term |[ <nabl2--occurrence-index-of> ~t ]|
 

  injection-to-pattern(e) = pattern-to-pattern(e)
  injection-to-pattern(e):
    CWld() -> Wld()
