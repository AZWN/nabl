module nabl2/runtime/analysis/log 

rules

  nabl2--collection-info(|msg) =
    where(nabl2--is-debug-collection-enabled)
  < log(|Info(), msg)
  + id

  nabl2--warn(|msg) =
    warn-msg(|msg)

  nabl2--error(|msg) =
    err-msg(|msg)

  nabl2--is-debug-collection-enabled =
    prim("SG_is_debug_collection_enabled")
