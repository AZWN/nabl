module statix/test/Result

imports

  statix/lang/Layout
  statix/lang/Common
  statix/lang/Core
  statix/lang/Sugar
  statix/lang/Deprecated
  statix/runtime/NoParse

template options

  keyword -/- [a-zA-Z0-9\_\-\']

context-free syntax

  TestResult.EvalResult = <
    <Solution>

    errors
      <{EditorMessage "\n"}*>

    warnings
      <{EditorMessage "\n"}*>

    notes
      <{EditorMessage "\n"}*>
  >

  Solution.Solution = <
    substitution
      <{SubstEntry "\n"}*>

    <Analysis>
  >

context-free syntax

  // special pretty print rule in statix/runtime/pp to support printing blob analyses too

  Analysis.Analysis = <
    analysis
      <ScopeGraph>
  >

context-free syntax

  SubstEntry = [[Var] |-> [Term]]
  
context-free syntax

  ScopeGraph = <
    scope graph
      <{ScopeEntry "\n"}*>
  >

  ScopeEntry = <
    <Scope><DataEntry> {
      <{EdgeEntry "\n"}*>
    }
  >

  DataEntry.None =
  DataEntry.Some = < : <Term>>

  EdgeEntry = <<Label> : <EntryValues>>

  EntryValues = {Term "\n"}*

context-free syntax

  EditorMessage = [[MessagePos] : [String]]

  MessagePos = "*"

lexical syntax

  String = ~[]
