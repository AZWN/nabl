module statix/test/statixtest

imports

  signatures/statix/test/-

  statix/lang/statics/-

rules

  [[ t@Test(c, sections) ^ (s) ]] :=
    new s_test, s_test -P-> s, @t.scope := s_test,
    new s_syn, s_test -LS-> s_syn,
    Sections[[ sections ^ (s_test, s_syn, s) ]],
    Constraint[[ c ^ (s_test) ]],
    This{()} <- s_test, This{()}.qname := "" !!.
