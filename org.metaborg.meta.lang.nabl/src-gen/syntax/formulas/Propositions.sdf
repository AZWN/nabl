module formulas/Propositions
imports terms/Terms terms/Vars

exports
  context-free syntax
    VarRef "==" Term                    -> Proposition {cons("Eq")}
    VarRef "=>" Pattern                 -> Proposition {cons("Match")}
    VarRef "has" "type" Var             -> Proposition {cons("HasProperty")}
    Term "has" "associated" "scope" Var -> Proposition {cons("HasScope")}

  context-free syntax
    CONTENTCOMPLETE -> Proposition {cons("COMPLETION-Proposition")}