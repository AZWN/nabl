module generation/messages

imports 
  
  include/NameBindingLanguage
  libstrc
  
  generation/-
  
rules
 
  formula-to-tasks:
    Message(kind, msg, t) -> TASK(<kind-to-strategy> kind, [msg], t)
  
  kind-to-strategy: Note()    -> "task-create-note"
  kind-to-strategy: Warning() -> "task-create-waening"
  kind-to-strategy: Error()   -> "task-create-error"
  