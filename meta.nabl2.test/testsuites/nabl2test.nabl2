module testsuites/nabl2test

imports

  testsuites/api/-
  testsuites/base/-
  testsuites/constraint-generation/-
  testsuites/relations-functions/-
  testsuites/known-problems/-
  testsuites/name-resolution/-
  testsuites/polymorphism/-
  testsuites/unification/-

signature

  namespaces
    Ns1
    Ns2

  name resolution
    labels P I
    order  D < P, D < I, I < P
    well-formedness P* I*

  relations
    reflexive, transitive, anti-symmetric sub : a * a {
      [+sub]
    }

  functions
  
    projectFirst: term -> term {
      (fst, _) -> fst
    }

    projectSecond: term -> term {
      (_, snd) -> snd
    }

rules

  init ^ ().
